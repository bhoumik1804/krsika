        },
        {
          "name": "Reactivate Mill",
          "request": { "method": "PATCH", "url": { "raw": "{{baseUrl}}/admin/mills/{{millId}}/reactivate", "host": ["{{baseUrl}}"], "path": ["admin", "mills", "{{millId}}", "reactivate"] } }
        },
        {
          "name": "Delete Mill",
          "request": { "method": "DELETE", "url": { "raw": "{{baseUrl}}/admin/mills/{{millId}}", "host": ["{{baseUrl}}"], "path": ["admin", "mills", "{{millId}}"] } }
        },
        {
          "name": "Bulk Delete Mills",
          "request": {
            "method": "DELETE",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": { "mode": "raw", "raw": "{\n  \"ids\": [\"id1\", \"id2\"]\n}" },
            "url": { "raw": "{{baseUrl}}/admin/mills/bulk", "host": ["{{baseUrl}}"], "path": ["admin", "mills", "bulk"] }
          }
        }
