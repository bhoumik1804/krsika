            "body": { "mode": "raw", "raw": "{\n  \"millName\": \"New Rice Mill\",\n  \"contact\": {\n    \"email\": \"newmill@example.com\",\n    \"phone\": \"9876543210\"\n  },\n  \"millInfo\": {\n    \"gstNumber\": \"22BBBBB0000B1Z6\",\n    \"panNumber\": \"BBBBB0000B\",\n    \"address\": \"456 Mill Road\",\n    \"city\": \"Delhi\",\n    \"state\": \"Delhi\",\n    \"pincode\": \"110001\"\n  }\n}" },
            "url": { "raw": "{{baseUrl}}/admin/mills", "host": ["{{baseUrl}}"], "path": ["admin", "mills"] }
          }
        },
        {
          "name": "Update Mill",
          "request": {
            "method": "PUT",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": { "mode": "raw", "raw": "{\n  \"millName\": \"Updated Mill Name\"\n}" },
            "url": { "raw": "{{baseUrl}}/admin/mills/{{millId}}", "host": ["{{baseUrl}}"], "path": ["admin", "mills", "{{millId}}"] }
          }
